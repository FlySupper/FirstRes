<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.dao.MessageMapper">

	<insert id="addmessage" parameterType="com.project.pojo.Message">
	    insert into message (m_text,m_name,m_phoneNumber,m_mailbox)
	    values(#{m_text},#{m_name},#{m_phoneNumber},#{m_mailbox})
	</insert>
   
</mapper>